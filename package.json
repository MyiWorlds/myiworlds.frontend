{
  "name": "myiworlds-apollo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/icons": "^1.0.0-beta.42",
    "apollo-boost": "^0.1.3",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-link-batch-http": "^1.2.1",
    "cross-env": "^5.1.4",
    "graphql": "^0.13.2",
    "graphql-anywhere": "^4.1.8",
    "graphql-tag": "^2.8.0",
    "jss": "^9.8.1",
    "jss-nested": "^6.0.1",
    "material-ui": "^1.0.0-beta.41",
    "react": "^16.2.0",
    "react-apollo": "^2.1.0",
    "react-dom": "^16.2.0",
    "react-jss": "^8.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "react-sizeme": "^2.4.0",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "start": "cross-env REACT_APP_API=http://localhost:8080 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "*.js": [
      "eslint --no-ignore --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "proxy": {
    "/graphql": {
      "target": "http://localhost:8080",
      "secure": false,
      "changeOrigin": true,
      "ws": true,
      "xfwd": true
    },
    "/login/google": {
      "target": "http://localhost:8080",
      "secure": true,
      "changeOrigin": true,
      "ws": true,
      "xfwd": true
    },
    "/login/clear": {
      "target": "http://localhost:8080",
      "secure": true,
      "changeOrigin": true,
      "ws": true,
      "xfwd": true
    }
  }
}
